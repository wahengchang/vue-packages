{
  "name": "@lazy-copilot/noscraper",
  "main": "./dist/noScraper.common.js",
  "module": "dist/noScraper.esm.js",
  "unpkg": "dist/noScraper.min.js",
  "browser": {
    "./sfc": "src/components/noScraper.vue"
  },
  "version": "0.1.28",
  "scripts": {
    "precommit": "lint-staged; npm test",
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name noScraper src/build.js",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "lint-staged": "^7.2.2",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "description": "## Project setup ``` npm install ```",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wahengchang/vue-packages.git"
  },
  "keywords": [
    "vue",
    "component",
    "phantomjs",
    "headless",
    "browser",
    "selenium",
    "prevent",
    "protection",
    "hack",
    "security"
  ],
  "author": "lazy-copilot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wahengchang/vue-packages/issues"
  },
  "homepage": "https://github.com/wahengchang/vue-packages#readme"
}
